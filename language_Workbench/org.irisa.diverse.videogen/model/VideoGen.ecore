    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="minMaxNotEqual" value="&#xA;&#x9;&#x9;&#x9;self.minDurationConstraint &lt;= self.maxDurationConstraint"/>
