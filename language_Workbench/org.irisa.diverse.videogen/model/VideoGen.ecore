      <details key="notIdentical" value="&#xA;&#x9;&#x9;&#x9;self.allInstances()->forAll(s1, s2 |&#xA;&#x9;&#x9;&#x9;&#x9;s1 &lt;> s2 implies s1.oclAsType(NamedElement).name &lt;> s2.oclAsType(NamedElement).name)"/>
