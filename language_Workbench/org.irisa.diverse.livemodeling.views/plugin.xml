<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.gemoc.executionframework.ui.category"
            name="Gemoc">
      </category>
      <view
            category="org.gemoc.executionframework.ui.category"
            class="org.irisa.diverse.livemodeling.views.durationConstraint.DurationView"
            icon="icons/IconeGemocModel-16.png"
            id="org.irisa.diverse.livemodeling.views.durationConstraint.DurationView"
            name="Duration constraint">
      </view>
	</extension>
	<extension
         point="org.eclipse.ui.views">
      <category
            id="org.gemoc.executionframework.ui.category"
            name="Gemoc">
      </category>
      <view
            category="org.gemoc.executionframework.ui.category"
            class="org.irisa.diverse.livemodeling.views.sequenceConstraint.SequenceView"
            icon="icons/IconeGemocModel-16.png"
            id="org.irisa.diverse.livemodeling.views.sequenceConstraint.SequenceView"
            name="Sequence number constraint">
      </view>
	</extension>
   <extension
         point="org.gemoc.gemoc_language_workbench.engine_addon_group">
         <AddonGroup
            Name="Sequential"
            id="Sequential.AddonGroup">
      </AddonGroup>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.sirius.ui.tools.perspective.modeling">
         <viewShortcut
               id="org.irisa.diverse.livemodeling.views.durationConstraint.DurationView">
         </viewShortcut>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.sirius.ui.tools.perspective.modeling">
         <viewShortcut
               id="org.irisa.diverse.livemodeling.views.sequenceConstraint.SequenceView">
         </viewShortcut>
      </perspectiveExtension>
   </extension>
   
   
  <extension point="org.gemoc.gemoc_language_workbench.engine_addon">
    <Addon
    	Class="org.irisa.diverse.livemodeling.views.ConstraintEngineAddon"
    	Default="true"
    	id="org.irisa.diverse.livemodeling.views.durationConstraint"
    	Name="Duration constraint"
    	ShortDescription="Constraint support dedicated to videoGen language"
    	openViewIds="org.irisa.diverse.livemodeling.views.durationConstraint.DurationView" />
  </extension>   
  <extension point="org.gemoc.gemoc_language_workbench.engine_addon">
    <Addon
    	Class="org.irisa.diverse.livemodeling.views.ConstraintEngineAddon"
    	Default="true"
    	id="org.irisa.diverse.livemodeling.views.sequenceConstraint"
    	Name="Sequence constraint"
    	ShortDescription="Video number"
    	openViewIds="org.irisa.diverse.livemodeling.views.sequenceConstraint.SequenceView" />
  </extension>
  
   <extension
         point="org.irisa.diverse.livemodeling.api.modelAdaptor">
      <modelAdaptor
            class="org.irisa.diverse.livemodeling.views.impl.VideoGenAdaptor">
      </modelAdaptor>
   </extension>
</plugin>
