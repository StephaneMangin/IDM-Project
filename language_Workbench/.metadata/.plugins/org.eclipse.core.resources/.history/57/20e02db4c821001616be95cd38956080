package org.irisa.diverse.videogen

import org.irisa.diverse.videogen.videoGen.aspects.visitors.*
import org.irisa.diverse.videogen.videoGen.aspects.*

language VideoGenS {
	syntax "platform:/resource//org.irisa.diverse.videogen/model/VideoGen.ecore"

}

language VideoGenL inherits VideoGenS {

	with VideoGenAspect
	with TransitionAspect
	with MandatoryAspect
	with OptionalAspect
	with AlternativesAspect
	with InitializeAspect
	with GenerateAspect
	with SequenceAspect
	with DelayAspect
	with VideoGenAspect
	with VideoGenDurationConstraintsAspect
	with MandatoryDurationConstraintsAspect
	with OptionalDurationConstraintsAspect
	with AlternativesDurationConstraintsAspect
	with SequenceDurationConstraintsAspect
	with VideoGenUserContraintsVisitor

}