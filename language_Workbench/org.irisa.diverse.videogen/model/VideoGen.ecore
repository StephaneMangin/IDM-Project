  </eAnnotations>
  <eClassifiers xsi:type="ecore:EClass" name="Mandatory" eSuperTypes="#//Sequence"/>
  <eClassifiers xsi:type="ecore:EClass" name="Initialize" eSuperTypes="#//Transition"/>
