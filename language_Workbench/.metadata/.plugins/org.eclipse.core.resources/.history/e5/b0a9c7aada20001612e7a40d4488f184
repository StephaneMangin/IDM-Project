package org.irisa.diverse.videogen.videogenl.adapters.videogenlmt.videoGen;

import fr.inria.diverse.melange.adapters.EObjectAdapter;
import org.eclipse.emf.ecore.EClass;
import org.irisa.diverse.videogen.videoGen.Optional;
import org.irisa.diverse.videogen.videogenl.adapters.videogenlmt.VideoGenLMTAdaptersFactory;

@SuppressWarnings("all")
public class OptionalAdapter extends EObjectAdapter<Optional> implements org.irisa.diverse.videogen.videogenlmt.videoGen.Optional {
  private VideoGenLMTAdaptersFactory adaptersFactory;
  
  public OptionalAdapter() {
    super(org.irisa.diverse.videogen.videogenl.adapters.videogenlmt.VideoGenLMTAdaptersFactory.getInstance());
    adaptersFactory = org.irisa.diverse.videogen.videogenl.adapters.videogenlmt.VideoGenLMTAdaptersFactory.getInstance();
  }
  
  @Override
  public String getName() {
    return adaptee.getName();
  }
  
  @Override
  public void setName(final String o) {
    adaptee.setName(o);
  }
  
  @Override
  public Boolean getActive() {
    return adaptee.getActive();
  }
  
  @Override
  public void setActive(final Boolean o) {
    adaptee.setActive(o);
  }
  
  @Override
  public Boolean getSelected() {
    return adaptee.getSelected();
  }
  
  @Override
  public void setSelected(final Boolean o) {
    adaptee.setSelected(o);
  }
  
  @Override
  public Integer getProbability() {
    return adaptee.getProbability();
  }
  
  @Override
  public void setProbability(final Integer o) {
    adaptee.setProbability(o);
  }
  
  @Override
  public org.irisa.diverse.videogen.videogenlmt.videoGen.Transition getNextTransition() {
    return () adaptersFactory.createAdapter(adaptee.getNextTransition(), eResource);
  }
  
  @Override
  public void setNextTransition(final org.irisa.diverse.videogen.videogenlmt.videoGen.Transition o) {
    if (o != null)
    	adaptee.setNextTransition(((org.irisa.diverse.videogen.videogenl.adapters.videogenlmt.videoGen.TransitionAdapter) o).getAdaptee());
    else adaptee.setNextTransition(null);
  }
  
  @Override
  public org.irisa.diverse.videogen.videogenlmt.videoGen.Video getVideo() {
    return () adaptersFactory.createAdapter(adaptee.getVideo(), eResource);
  }
  
  @Override
  public void setVideo(final org.irisa.diverse.videogen.videogenlmt.videoGen.Video o) {
    if (o != null)
    	adaptee.setVideo(((org.irisa.diverse.videogen.videogenl.adapters.videogenlmt.videoGen.VideoAdapter) o).getAdaptee());
    else adaptee.setVideo(null);
  }
  
  @Override
  public void execute() {
    adaptee.execute();
  }
  
  @Override
  public org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGen getVideoGen() {
    org.irisa.diverse.videogen.videoGen.aspects.TransitionAspect.videoGen(adaptee);
  }
  
  @Override
  public void setVideoGen(final org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGen videoGen) {
    org.irisa.diverse.videogen.videoGen.aspects.TransitionAspect.videoGen(adaptee, ((org.irisa.diverse.videogen.videogenl.adapters.videogenlmt.videoGen.VideoGenAdapter) videoGen).getAdaptee()
    );
  }
  
  @Override
  public Boolean getExecuted() {
    return org.irisa.diverse.videogen.videoGen.aspects.TransitionAspect.executed(adaptee);
  }
  
  @Override
  public void setExecuted(final Boolean executed) {
    org.irisa.diverse.videogen.videoGen.aspects.TransitionAspect.executed(adaptee, executed
    );
  }
  
  @Override
  public Boolean getCallnextTransition() {
    return org.irisa.diverse.videogen.videoGen.aspects.TransitionAspect.callnextTransition(adaptee);
  }
  
  @Override
  public void setCallnextTransition(final Boolean callnextTransition) {
    org.irisa.diverse.videogen.videoGen.aspects.TransitionAspect.callnextTransition(adaptee, callnextTransition
    );
  }
  
  protected final static String NAME_EDEFAULT = null;
  
  protected final static Boolean ACTIVE_EDEFAULT = java.lang.Boolean.TRUE;
  
  protected final static Boolean SELECTED_EDEFAULT = java.lang.Boolean.FALSE;
  
  protected final static Boolean EXECUTED_EDEFAULT = null;
  
  protected final static Boolean CALLNEXT_TRANSITION_EDEFAULT = null;
  
  protected final static Integer PROBABILITY_EDEFAULT = new Integer(0);
  
  @Override
  public EClass eClass() {
    return org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.eINSTANCE.getOptional();
  }
  
  @Override
  public Object eGet(final int featureID, final boolean resolve, final boolean coreType) {
    switch (featureID) {
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__NAME:
    		return getName();
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__NEXT_TRANSITION:
    		return getNextTransition();
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__ACTIVE:
    		return getActive();
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__SELECTED:
    		return getSelected();
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__VIDEO_GEN:
    		return getVideoGen();
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__EXECUTED:
    		return getExecuted();
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__CALLNEXT_TRANSITION:
    		return getCallnextTransition();
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__VIDEO:
    		return getVideo();
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__PROBABILITY:
    		return getProbability();
    }
    
    return super.eGet(featureID, resolve, coreType);
  }
  
  @Override
  public boolean eIsSet(final int featureID) {
    switch (featureID) {
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__NAME:
    		return getName() != NAME_EDEFAULT;
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__NEXT_TRANSITION:
    		return getNextTransition() != null;
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__ACTIVE:
    		return getActive() != ACTIVE_EDEFAULT;
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__SELECTED:
    		return getSelected() != SELECTED_EDEFAULT;
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__VIDEO_GEN:
    		return getVideoGen() != null;
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__EXECUTED:
    		return getExecuted() != EXECUTED_EDEFAULT;
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__CALLNEXT_TRANSITION:
    		return getCallnextTransition() != CALLNEXT_TRANSITION_EDEFAULT;
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__VIDEO:
    		return getVideo() != null;
    	case org.irisa.diverse.videogen.videogenlmt.videoGen.VideoGenPackage.OPTIONAL__PROBABILITY:
    		return getProbability() != PROBABILITY_EDEFAULT;
    }
    
    return super.eIsSet(featureID);
  }
  
